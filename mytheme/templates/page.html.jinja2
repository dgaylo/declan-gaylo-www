{% extends "includes/base.html.jinja2" %}
{% block title %}{{ page.title }} - {{ SITENAME }}{%endblock%}

{% block canonical_rel %}
  <link rel="canonical" href="{{ SITEURL }}/{{ page.url }}">
{% endblock %}

{% block breadcrumb %}
<ol class="breadcrumb">
  <li class="breadcrumb-item"><a href="{{ SITEURL }}/">Home</a></li>
  <li class="breadcrumb-item active">{{ page.slug | capitalize }}</li>
</ol>
{% endblock %}

{% block content %}
    <h1>{{ page.title }}</h1>
    {{ page.content }}
{% endblock %}